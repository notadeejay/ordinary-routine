  \n  let productDetails = this.state.products.find(product => product.id === productId)\n \n  this.props.addToRoutine(routine, productDetails)\n}\n  render() {\n  \n  const renderButtons = (product) => {\n    let timeOfDay = product.am_pm\n    if (timeOfDay.includes(\"AM\") && timeOfDay.includes(\"PM\")) {\n      return <React.Fragment><button onClick={this.handleClick} routine-id=\"AM\" product-id={product.id} className=\"add__morning\">Add to AM Routine</button> \n      <button onClick={this.handleClick} routine-id=\"PM\" product-id={product.id} className=\"add__evening\">Add to PM Routine</button></React.Fragment>\n    } else if (timeOfDay.includes(\"AM\")) {\n      return <button onClick={this.handleClick} routine-id=\"AM\" product-id={product.id} className=\"add__morning\">Add to AM Routine</button>\n    } else {\n      return <button onClick={this.handleClick} routine-id=\"PM\" product-id={product.id} className=\"add__evening\">Add to PM Routine</button>\n    }\n  }\n\n    return (\n      <div className=\"product-modal__backdrop\" onClick={this.props.close}>\n      <div className=\"product-modal\">\n        {this.state.products.map((product) => {\n           if (product.id == this.props.productId) {\n            return <div  className=\"product-details\">\n              <h2>{product.name}</h2>\n              <p>{product.description}</p>\n              <span className=\"product__category\">{product.category}</span>\n              <div className=\"additional-details\">\n            <div className=\"targets\">\n               <p>Targets</p>\n                <ul className=\"product__targets\">{product.targets.map((target) => {\n                  return <li>{target}</li>})}\n                </ul>\n            </div>\n            <div>\n              <p>When to Use</p>\n              <ul className=\"product__use\"><li>{product.when_to_use}</li></ul>\n            </div>\n          </div>\n              <div className=\"routine-controls\">\n                {renderButtons(product)}\n              </div>\n            </div>\n           }\n        })}\n        \n\n      </div>\n      </div>\n     \n    );\n  }\n}\n\nexport default ProductModal;","/Users/amymansell/Desktop/side-projects/ordinary-routine/client/src/index.js",[],{"ruleId":"62","severity":1,"message":"63","line":4,"column":8,"nodeType":"64","messageId":"65","endLine":4,"endColumn":19},{"ruleId":"66","severity":1,"message":"67","line":31,"column":15,"nodeType":"68","messageId":"69","endLine":31,"endColumn":17},{"ruleId":"70","severity":1,"message":"71","line":74,"column":9,"nodeType":"72","endLine":74,"endColumn":68},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"70","severity":1,"message":"71","line":14,"column":32,"nodeType":"72","endLine":14,"endColumn":63},{"ruleId":"70","severity":1,"message":"71","line":15,"column":11,"nodeType":"72","endLine":15,"endColumn":44},{"ruleId":"70","severity":1,"message":"71","line":18,"column":16,"nodeType":"72","endLine":18,"endColumn":48},{"ruleId":"70","severity":1,"message":"71","line":20,"column":16,"nodeType":"72","endLine":20,"endColumn":49},{"ruleId":"73","replacedBy":"77"},{"ruleId":"75","replacedBy":"78"},{"ruleId":"73","replacedBy":"79"},{"ruleId":"75","replacedBy":"80"},{"ruleId":"62","severity":1,"message":"63","line":3,"column":8,"nodeType":"64","messageId":"65","endLine":3,"endColumn":19},{"ruleId":"66","severity":1,"message":"67","line":15,"column":32,"nodeType":"68","messageId":"69","endLine":15,"endColumn":34},{"ruleId":"70","severity":1,"message":"71","line":16,"column":16,"nodeType":"72","endLine":16,"endColumn":31},{"ruleId":"70","severity":1,"message":"71","line":18,"column":16,"nodeType":"72","endLine":18,"endColumn":32},{"ruleId":"62","severity":1,"message":"81","line":2,"column":8,"nodeType":"64","messageId":"65","endLine":2,"endColumn":11},{"ruleId":"82","severity":1,"message":"83","line":38,"column":44,"nodeType":"84","messageId":"85","endLine":38,"endColumn":46},{"ruleId":"6